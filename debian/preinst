#!/bin/sh

set -e

case "$1" in
    install|upgrade)
	if [ -d /usr/share/doc/bash/completion-contrib ] && [ ! -h /usr/share/doc/bash/completion-contrib ]; then
	    rm -rf /usr/share/doc/bash/completion-contrib
	fi
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0


