proc setup {} {
    save_env
}


proc teardown {} {
    assert_env_unmodified
}


setup


assert_complete_any "kill 1"
sync_after_int


assert_complete [get_signals] "kill -s "
sync_after_int


set expected [get_signals -]
lappend expected "-l" "-s"
assert_complete [lsort -unique $expected] "kill -"
sync_after_int


teardown
