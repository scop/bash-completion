#!/bin/sh

case "${2-}" in
    b|ba|bar)
        echo bar
        ;;
    cont1*)
        echo cont10
        echo cont11\\
        ;;
    f|fo|foo)
        echo foo
        ;;
    l)
        echo line\\
        echo two
        echo long
        ;;
    li*)
        echo line\\
        echo two
        ;;
    lo*)
        echo long
        ;;
    *)
        echo bar
        echo foo
        ;;
esac
