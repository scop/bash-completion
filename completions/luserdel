# luserdel(1) completion                                   -*- shell-script -*-

_luserdel()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        --help|--usage|-!(-*)[?]*)
            return
            ;;
    esac

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $(compgen -W '$(_parse_help "$1")' -- "$cur") )
        return
    fi

    COMPREPLY=( $(compgen -u -- "$cur") )
} &&
complete -F _luserdel luserdel

# ex: filetype=sh
