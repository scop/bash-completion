# pwd(1) completion                                        -*- shell-script -*-

_pwd()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        --help | --version)
            return
            ;;
    esac

    COMPREPLY=($(compgen -W '$(_parse_usage help "-s $1")' -- "$cur"))
} &&
    complete -F _pwd pwd

# ex: filetype=sh
