# pm-powersave(8) completion                               -*- shell-script -*-

_comp_cmd_pm_powersave()
{
    local cur prev words cword comp_args
    _comp_initialize -- "$@" || return

    COMPREPLY=($(compgen -W "true false" -- "$cur"))
} &&
    complete -F _comp_cmd_pm_powersave pm-powersave

# ex: filetype=sh
