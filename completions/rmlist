# mailman rmlist completion                                -*- shell-script -*-

_rmlist()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $(compgen -W '--archives --help' -- "$cur") )
    else
        _xfunc list_lists _mailman_lists
    fi

} &&
complete -F _rmlist rmlist

# ex: filetype=sh
