# Slackware rpm2tgz completion                             -*- shell-script -*-

_rpm2tgz()
{
    local cur prev words cword
    _init_completion || return

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '-s -S -n -r -d -c' -- "$cur"))
        return
    fi

    _filedir "rpm"
} &&
    complete -F _rpm2tgz rpm2tgz rpm2txz rpm2targz

# ex: filetype=sh
