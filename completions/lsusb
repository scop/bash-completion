# lsusb(8) completion                                      -*- shell-script -*-

_lsusb()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        --help|--version|-!(-*)@([sD]|[hV]*))
            return
            ;;
    esac

    if [[ $cur == -* ]]; then
        COMPREPLY=( $( compgen -W '$( _parse_help "$1" )' -- "$cur" ) )
    fi
} &&
complete -F _lsusb lsusb

# ex: filetype=sh
