# xhost(1) completion                                      -*- shell-script -*-

_xhost ()
{
    local cur prev words cword
    _init_completion || return

    case $cur in
        +*) _known_hosts_real -p+ -- "${cur:1}" ;;
        -*) _known_hosts_real -p- -- "${cur:1}" ;;
        *)  _known_hosts_real -- "$cur" ;;
    esac
} &&
complete -F _xhost xhost

# ex: filetype=sh
