# cfagent completion                                       -*- shell-script -*-

_cfagent()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -f|--file)
            _filedir
            return
            ;;
    esac

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $(compgen -W '$(_parse_help "$1")' -- "$cur") )
    fi
} &&
complete -F _cfagent cfagent

# ex: filetype=sh
