# lua(1) completion                                        -*- shell-script -*-

_lua()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -e|-l|-v|-)
            return
            ;;
    esac

    if [[ $cur == -* ]]; then
        COMPREPLY=( $(compgen -W "$(_parse_help "$1")" -- "$cur") )
        return
    fi

    _filedir 'l@(ua|?(ua)c)'
} &&
complete -F _lua lua

# ex: filetype=sh
