# mailman qrunner completion                               -*- shell-script -*-

_comp_cmd_qrunner()
{
    local cur prev words cword split comp_args
    _comp_initialize -s -- "$@" || return

    $split && return

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '--runner --once --list --verbose --subproc
            --help' -- "$cur"))
    fi

} &&
    complete -F _comp_cmd_qrunner qrunner

# ex: filetype=sh
