# mailman inject completion                                -*- shell-script -*-

_inject()
{
    local cur prev words cword split
    _init_completion -s || return

    case $prev in
        -l | --listname)
            _xfunc list_lists _mailman_lists
            return
            ;;
    esac

    $split && return

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '--listname --queue --help' -- "$cur"))
    else
        _filedir
    fi

} &&
    complete -F _inject inject

# ex: filetype=sh
