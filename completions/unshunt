# mailman unshunt completion                               -*- shell-script -*-

_unshunt()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $(compgen -W '$(_parse_help "$1")' -- "$cur") )
    else
        _filedir -d
    fi

} &&
complete -F _unshunt unshunt

# ex: filetype=sh
