# mailman unshunt completion                               -*- shell-script -*-

_comp_cmd_unshunt()
{
    local cur prev words cword comp_args
    _comp_initialize -- "$@" || return

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '$(_parse_help "$1")' -- "$cur"))
    else
        _filedir -d
    fi

} &&
    complete -F _comp_cmd_unshunt unshunt

# ex: filetype=sh
