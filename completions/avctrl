# avctrl completion                                        -*- shell-script -*-

_avctrl()
{
    local cur prev words cword
    _init_completion || return

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '--help --quiet' -- "$cur"))
    else
        local args
        _count_args
        if ((args == 1)); then
            COMPREPLY=($(compgen -W 'discover switch' -- "$cur"))
        fi
    fi
} &&
    complete -F _avctrl avctrl

# ex: filetype=sh
