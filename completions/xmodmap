# xmodmap(1) completion                                    -*- shell-script -*-

_xmodmap()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -display|-e)
            return
            ;;
    esac

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $(compgen -W '$(_parse_help "$1" -help)' -- "$cur") )
        return
    fi

    _filedir
} &&
complete -F _xmodmap xmodmap

# ex: filetype=sh
