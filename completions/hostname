# hostname(1) completion                                   -*- shell-script -*-

_hostname()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        --help|--version|-!(-*)[hV])
            return
            ;;
        --file|-!(-*)F)
            _filedir
            return
            ;;
    esac

    [[ $cur == -* ]] && \
        COMPREPLY=( $(compgen -W '$(_parse_help "$1")' -- "$cur") )
} &&
complete -F _hostname hostname

# ex: filetype=sh
