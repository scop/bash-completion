# radvdump(8) completion                                   -*- shell-script -*-

_radvdump()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -h|--help|-v|--version)
            return
            ;;
        -d|--debug)
            COMPREPLY=( $(compgen -W '{1..4}' -- "$cur") )
            return
            ;;
    esac

    COMPREPLY=( $(compgen -W '$(_parse_usage "$1" --help)' -- "$cur") )
} &&
complete -F _radvdump radvdump

# ex: filetype=sh
